{
    "name": "mach3builders/mach3queue",
    "description": "A php queue system",
    "autoload": {
        "psr-4": {
            "Mach3queue\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "require": {
        "illuminate/database": "^10.43"
    },
    "require-dev": {
        "pestphp/pest": "^2.33"
    },
    "bin": [
        "mach3worker"
    ],
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
