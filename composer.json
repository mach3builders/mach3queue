{
    "name": "mach3builders/mach3queue",
    "description": "A php queue system",
    "autoload": {
        "psr-4": {
            "Mach3queue\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "require": {
        "illuminate/database": "^10.43",
        "symfony/process": "^7.0",
        "ext-pcntl": "*",
        "ext-posix": "*",
        "ext-dom": "*",
        "opis/closure": "^3.6"
    },
    "require-dev": {
        "pestphp/pest": "^2.33",
        "mockery/mockery": "^1.6",
        "spatie/ray": "^1.41"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "bin": [
        "queue"
    ]
}
